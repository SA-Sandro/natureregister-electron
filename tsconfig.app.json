{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": ["node", "vite/client"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "lib": ["ES2020", "DOM"]
  },
  "include": ["server/**/*", "src/**/*", "vite.config.*", "vitest.config.*"]
}
